{
    "name": "indexzer0/eloquent-filtering",
    "description": "Powerful eloquent filtering",
    "keywords": [
        "IndexZer0",
        "laravel",
        "eloquent-filtering",
        "Nested",
        "Nesting",
        "Column",
        "Relationships",
        "Eloquent",
        "Filtering",
        "Filters",
        "Filter",
        "Custom Filters",
        "Sorting",
        "Sort"
    ],
    "homepage": "https://github.com/indexzer0/eloquent-filtering",
    "license": "MIT",
    "authors": [
        {
            "name": "IndexZer0",
            "email": "24657691+IndexZer0@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||v3.1.0",
        "orchestra/testbench": "^10.4.0",
        "pestphp/pest": "^3.0.0",
        "pestphp/pest-plugin-arch": "^3.0.0",
        "pestphp/pest-plugin-laravel": "^3.0.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "IndexZer0\\EloquentFiltering\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IndexZer0\\EloquentFiltering\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-eloquent-filtering --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IndexZer0\\EloquentFiltering\\EloquentFilteringServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
